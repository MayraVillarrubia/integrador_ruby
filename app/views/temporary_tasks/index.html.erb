<p id="notice"><%= notice %></p>

<h1>Temporary Tasks</h1>

<table>
  <thead>
    <tr>
      <th>Start date</th>
      <th>End date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @temporary_tasks.each do |temporary_task| %>
      <tr>
        <td><%= temporary_task.start_date %></td>
        <td><%= temporary_task.end_date %></td>
        <td><%= link_to 'Show', temporary_task %></td>
        <td><%= link_to 'Edit', edit_temporary_task_path(temporary_task) %></td>
        <td><%= link_to 'Destroy', temporary_task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Temporary Task', new_temporary_task_path %>
